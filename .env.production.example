# Production Environment Variables for Learnnect
# Copy this file to .env.production and fill in your actual values

# Enhanced AI Backend URL (your Python backend)
VITE_API_URL=https://your-ai-backend.com

# Firebase Configuration
VITE_FIREBASE_API_KEY=your-production-firebase-api-key
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your-sender-id
VITE_FIREBASE_APP_ID=your-app-id

# Google Services (Optional)
VITE_GOOGLE_SHEETS_API_KEY=your-google-sheets-api-key
VITE_GOOGLE_SHEET_ID=your-google-sheet-id
VITE_GOOGLE_APPS_SCRIPT_URL=your-google-apps-script-url

# Analytics (Optional)
VITE_GA_MEASUREMENT_ID=G-XXXXXXXXXX

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_CHATBOT=true
VITE_ENABLE_VOICE_FEATURES=true
